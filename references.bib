@article{tukeyConfiguralPolysampling1987,
  title = {Configural {{Polysampling}}},
  volume = {29},
  issn = {00361445},
  abstract = {In dealing with the (necessary) oversimplifications, it can be important to distinguish models, toys and leading cases (the latter are emphasized here). Analyzing data involves both stochastic and functional models. We should do whatever we can to avoid having to trust the correctness of a single stochastic model. Narrow specifications are usually toys. Very broad specifications (e.g., nonparametric or distribution-free specifications) are usually wasteful and, more importantly, often keep us from seeking stringency. Robustness--seeking good performance over a reasonably wide range of stochastic alternatives--is the middle way. Two ways of formulating "a reasonably wide range"--an asymptotic approach and a few-specified leading case approach-have so far led to similar choices. If and when they disagree, the latter is to be preferred.A particular estimator of location for a particular specification is classically assessed in such terms as mean square error, variance and pseudovariance, which prove to be mutually supporting. Configurations are equivalence classes of samples, equivalent under a linear transformation. Requiring equivariance within each configuration is an effective way of ruling out possible effects of preconceptions. Specifications can be dangerously unrealistic by not being bland.We can arrange to use a single set of samples as sets of samples from different distributions by using different sets of weights. Such polysampling can be extended to configurations. Averaging over samples can often usefully begin by going first to configurations by using numerical integration. The second stage ordinarily involves experimental sampling.Pitman gave the form of the minimum variance equivariant estimate 46 years ago (Biometrika, 30 (1939), pp. 391--421). For the simpler problems we can now afford to do the necessary numerical integrations. Pitman's results can be extended to the minimization of variance for two (or more) specifications, when the answer becomes a parametric family of estimates, which can be found in terms of chosen linear combinations of the two (or more) variances.Many of us realize that minimizing exactly for one criterion often leads to unsatisfactory performance for other criteria. Minimizing exactly for two criteria is not as dangerous, but has similar difficulties. This is illustrated, qualitatively, for location with \$n = 20\$. The bioptimal estimator, while extremely valuable in making clear whan can and cannot be done, is probably not the estimator of final practical choice for location. George Easton has been led by configural polysampling ideas to a different approach, which seems most promising (unpublished Ph.D. thesis, Princeton Univ., 1985).The logical flow of the discussion of the location problem includes important elements of insight and factual knowledge, both before and after the formal optimization. Those who design consumer products, be they milk bottles or estimators, cannot be content with the results of formal optimization. Techniques of double (and multiple) optimization deserve more attention than SIAM members seem to have given them.},
  language = {English},
  number = {1},
  journal = {SIAM Review},
  doi = {http://dx.doi.org.libproxy.mit.edu/10.1137/1029001},
  author = {Tukey, John W.},
  month = mar,
  year = {1987},
  keywords = {Mathematics,Technology: Comprehensive Works},
  pages = {20},
  copyright = {[Copyright] \textcopyright{} 1987 Society for Industrial and Applied Mathematics}
}

